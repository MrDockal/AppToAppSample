<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns:tizen="http://tizen.org/ns/widgets"
        xmlns="http://www.w3.org/ns/widgets"
        id="http://yourdomain/App2AppCaller"
        version="1.0.0"
        viewmodes="maximized">
    <tizen:application id="demoapp002.App2AppCaller"
                       package="demoapp002"
                       required_version="2.3"/>
    <content src="index.html"/>
    <feature name="http://tizen.org/feature/screen.size.all"/>
    <icon src="icon.png"/>
    <name>App2AppCaller</name>
    <tizen:privilege name="http://tizen.org/privilege/application.launch"/>
    <tizen:privilege name="http://tizen.org/privilege/tv.inputdevice"/>
    <tizen:profile name="tv"/>
    <tizen:setting screen-orientation="auto-rotation"
                   background-support="enable"
                   encryption="disable"
                   hwkey-event="enable"/>
</widget>
